    <input value="Linki Kopyala" id="copyLink" type="button" class="btn btn-success">

                <div class="chat-panel-start-chat">
                    <input type="text" id="getName" placeholder="Adınız" autocomplete="off">
                    <input type="button" id="startChat" class="btn"  value="Sohbet Başlat">
                </div>
                <div class="chat">
