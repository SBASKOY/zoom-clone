   <script src="script.js" defer></script>
  <script src="sharescreen.js" defer></script>
  <script src="filesytem.js" defer></script>
  <title>Chat Room</title>
  <style>

  </style>
</head>

<body>
  <div class="mycontainer">

    <div class="chat">
      <div>
        <input type="button" id="copyLink" value="Link Kopyala" class="btn btn-success">
      </div>
      <div class="start-chat">
        <input type="text" id="getName" placeholder="Adınız">
        <input type="button" id="startChat" class="btn btn-primary"  value="Sohbet Başlat">
      </div>
      <div class="chat-visible" id="chatDiv">
      
        <div id="chatpanel" style="overflow-y: auto;height: 75%">

        </div>
        
  
        <form action="">
          <input placeholder="Mesaj yolla" id="m" autocomplete="off" />
          <button>Gönder</button>
        </form>
      </div>


    </div>

    <div id="video-grid" class="video-grid">
     
      <canvas id="canvas" style="display: none;"></canvas>
      <div style="display:none;" id="toolbar">
        <button id="yellow" style="background-color: yellow;"></button>
        <button id="red" style="background-color: red;"></button>
        <button id="blue" style="background-color: blue;"></button>
        <button id="green" style="background-color: green;"></button>
        <button id="brown" style="background-color:brown;"></button>
        <button id="purple" style="background-color: purple;"></button>
      </div>
      <div>
      
      </div>
      <div id="firstElem">
       
      </div>


      <div id="video-grid1" class="video-grid1">
       
      </div>
    </div>
    <div class="setting">
      <img src="images/drawing.svg" width="50" height="50" id="shareCanvas">
      <img src="images/stop.svg" width="50" style="display: none;" height="50" id="stopCanvas">
      <img src="images/share.svg" width="50" height="50" id="startShare">
      <img src="images/stop.svg" width="50" style="display: none;" height="50" id="stopShare">
      <img src="images/restore.svg" width="50" height="50" style="display: none;" id="reset">
    </div>


  </div>
  <script>
    document.getElementById("roomId").innerText = ROOM_ID;
   

  </script>

</body>

</html>